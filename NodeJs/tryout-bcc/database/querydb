CREATE TABLE SKOR (
    ID INTEGER NOT NULL AUTO_INCREMENT PRIMARY KEY,
    ID_AKUN INTEGER NOT NULL,
    SKOR DOUBLE
);

CREATE TABLE UNIVERSITAS (
    ID INTEGER NOT NULL AUTO_INCREMENT PRIMARY KEY,
    NAMA VARCHAR(66) NOT NULL
);

CREATE TABLE JURUSAN (
    ID INTEGER NOT NULL AUTO_INCREMENT PRIMARY KEY,
    ID_UNIVERSITAS INTEGER NOT NULL REFERENCES UNIVERSITAS(ID),
    NAMA VARCHAR(66) NOT NULL
);

CREATE TABLE TIPE_MEMBER (
    ID INTEGER NOT NULL AUTO_INCREMENT PRIMARY KEY,
    NAMA VARCHAR(10) NOT NULL
);

CREATE TABLE TIPE_SOAL (
    ID INTEGER NOT NULL AUTO_INCREMENT PRIMARY KEY,
    NAMA VARCHAR(50) NOT NULL
);

CREATE TABLE SOAL (
    ID INTEGER NOT NULL AUTO_INCREMENT PRIMARY KEY,
    KONTEN TEXT NOT NULL,
    JAWABAN CHAR(5) NOT NULL,
    PENJAWAB_SALAH INTEGER,
    BOBOT DOUBLE,
    ID_TIPE_SOAL INTEGER REFERENCES TIPE_SOAL(ID)
);

CREATE TABLE AKUN (
    ID INTEGER NOT NULL AUTO_INCREMENT PRIMARY KEY,
    NAMA VARCHAR(50),
    EMAIL VARCHAR(50) NOT NULL,
    PASSWORD VARCHAR(100) NOT NULL,
    STATUS_BAYAR INTEGER,
    ID_TIPE_MEMBER INTEGER NOT NULL REFERENCES TIPE_MEMBER(ID),
    ID_UNIVERSITAS INTEGER NOT NULL REFERENCES UNIVERSITAS(ID),
    ID_JURUSAN INTEGER NOT NULL REFERENCES JURUSAN(ID),
    ID_SKOR INTEGER NOT NULL REFERENCES SKOR(ID)
);

